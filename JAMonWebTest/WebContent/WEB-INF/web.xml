<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>JAMonWebTest</display-name>
  <welcome-file-list>    
    <welcome-file>login.jsp</welcome-file>    
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>JAmomServlet</display-name>
    <servlet-name>JAmomServlet</servlet-name>
    <servlet-class>com.cb.servlet.JAmomServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JAmomServlet</servlet-name>
    <url-pattern>/JAmomServlet</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>JAmomServletFilter</filter-name>
    <filter-class>com.cb.servlet.JAmomServletFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>JAmomServletFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.cb.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
</web-app>